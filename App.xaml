<Application x:Class="AristaNetworkManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AristaNetworkManager"
             xmlns:views="clr-namespace:AristaNetworkManager.Views"
             xmlns:converters="clr-namespace:AristaNetworkManager.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <views:BoolToColorConverter x:Key="BoolToColorConverter" />
            <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
